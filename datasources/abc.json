          },
          "tableStyle": {
            "dataVariable": "State",
            "timeColumn": null
          }
        },
        {
          "name": "ABC Photo Stories (Real-Time View)",
          "type": "csv",
          "url": "test/localphotostories20092014.csv",
          "featureInfoTemplate": {
            "name": "{{{Title}}}, {{{Date}}}",
            "template": "<div><h3 style=\"margin-top: 0px;\">{{Title}}</h3><p>{{Place}}, {{Date}}</p><p><figure><img src='{{Primary image}}'><figcaption>{{Primary image caption}}</figcaption></figure></p><p style=\"text-align: center\"><a target='_blank' href={{URL}}>Read The Full Story</a></p></div>"
          },
          "tableStyle": {
            "dataVariable": "State"
          }
        }
      ]
    }
  ]
