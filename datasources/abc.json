{
  "catalog": [
    {
      "name": "ABC Photos",
      "type": "group",
      "isOpen": true,
      "items": [
        {
          "name": "ABC Photo Stories (2009-2014)",
          "isEnabled": true,
          "isShown": true,
          "type": "csv",
          "url": "test/localphotostories20092014.csv",
          "featureInfoTemplate": {
            "name": "{{{Title}}}, {{{Date}}}",
